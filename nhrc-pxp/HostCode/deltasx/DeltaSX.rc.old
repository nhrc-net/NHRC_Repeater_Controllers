Icon2 ICON "n.ico"
Icon3 ICON "GE.ico"

"DeltaSX" MENU 
BEGIN
 POPUP "&File"
 BEGIN
  MENUITEM "&New", FILE_NEW
  MENUITEM "&Open...", FILE_LOAD
  MENUITEM "&Save", FILE_SAVE
  MENUITEM "Save &As...", FILE_SAVE_AS
  MENUITEM "&Print", FILE_PRINT
  MENUITEM "E&xit", FILE_EXIT
 END

 POPUP "&Pod"
 BEGIN
 MENUITEM "&Get Program from Pod", POD_GET
 MENUITEM "&Put Program into Pod", POD_PUT
 MENUITEM "&Read device in pod", POD_READ
 MENUITEM "&Write device in pod", POD_WRITE
 END

 POPUP "&Help", HELP
 BEGIN
 MENUITEM "&About...", HELP_ABOUT
 END
END

